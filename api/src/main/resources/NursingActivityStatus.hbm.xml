<?xml version="1.0" ?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.nursingactivity.model">
  <class name="NursingActivityStatus" table="nursing_activity_status">
    <id name="activityStatusId" type="java.lang.Integer" column="nursing_activity_status_id">
      <generator class="native">
        <param name="sequence">nursing_activity_status_id</param>
      </generator>
    </id>
    <property name="status" type="java.lang.String" column="status" not-null="true"/>
  </class>
</hibernate-mapping>
